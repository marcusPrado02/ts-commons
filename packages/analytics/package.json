{
  "name": "@acme/analytics",
  "version": "0.1.0",
  "description": "Analytics event tracking with Segment, Mixpanel, GA4, custom dimensions, and funnel tracking for @acme/ts-commons",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src --ext .ts --max-warnings 0"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "analytics",
    "tracking",
    "segment",
    "mixpanel",
    "ga4",
    "funnel"
  ]
}
