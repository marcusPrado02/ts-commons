{
  "name": "@acme/data-pipeline",
  "version": "0.1.0",
  "description": "ETL framework, stream/batch processing, data validation and DLQ for @acme/ts-commons",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src --ext .ts --max-warnings 0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "etl",
    "pipeline",
    "stream",
    "batch",
    "data-pipeline",
    "dlq",
    "data-validation"
  ]
}
