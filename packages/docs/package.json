{
  "name": "@acme/docs",
  "version": "0.1.0",
  "description": "Story-driven interactive documentation for @acme TypeScript components",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src --ext .ts --max-warnings 0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "docs",
    "storybook",
    "stories",
    "ddd",
    "documentation",
    "interactive"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcusPrado02/ts-commons.git"
  },
  "bugs": "https://github.com/marcusPrado02/ts-commons/issues",
  "homepage": "https://github.com/marcusPrado02/ts-commons/tree/main/packages/docs#readme",
  "files": [
    "dist",
    "src",
    "README.md"
  ]
}
