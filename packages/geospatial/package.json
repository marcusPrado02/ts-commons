{
  "name": "@acme/geospatial",
  "version": "0.1.0",
  "description": "Geospatial data: PostGIS integration, coordinate systems, distance calculations, geofencing, spatial queries and GeoJSON support for @acme/ts-commons",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src --ext .ts --max-warnings 0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "geospatial",
    "postgis",
    "geojson",
    "geofencing",
    "haversine",
    "coordinates"
  ],
  "license": "MIT"
}
