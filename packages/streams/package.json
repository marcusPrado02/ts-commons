{
  "name": "@acme/streams",
  "version": "0.1.0",
  "description": "Stream processing: observable streams, operators, backpressure, merging/splitting and windowing for @acme/ts-commons",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src --ext .ts --max-warnings 0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "streams",
    "observable",
    "backpressure",
    "windowing",
    "operators"
  ],
  "license": "MIT"
}
