{
  "name": "@acme/web-express",
  "version": "0.1.0",
  "description": "Express.js adapter for ts-commons platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "express",
    "adapter",
    "clean-architecture",
    "middleware",
    "typescript"
  ],
  "author": "ACME Engineering",
  "license": "MIT",
  "dependencies": {
    "@acme/kernel": "workspace:*",
    "@acme/application": "workspace:*",
    "@acme/errors": "workspace:*",
    "@acme/observability": "workspace:*",
    "@acme/web": "workspace:*",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcusPrado02/ts-commons.git"
  },
  "bugs": "https://github.com/marcusPrado02/ts-commons/issues",
  "homepage": "https://github.com/marcusPrado02/ts-commons/tree/main/packages/web-express#readme"
}
