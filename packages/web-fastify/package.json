{
  "name": "@acme/web-fastify",
  "version": "0.1.0",
  "description": "Fastify adapter for ts-commons platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
